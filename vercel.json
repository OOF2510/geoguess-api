{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/warm/pano",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/warm/images",
      "schedule": "*/15 * * * *"
    }
  ]
}
